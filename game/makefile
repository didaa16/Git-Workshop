prog:main.o init.o text.o start.o option.o credit.o perso.o perso2.o single.o multi.o menu.o fonction.o persoo.o persoo2.o
	gcc main.o init.o text.o start.o option.o credit.o perso.o perso2.o single.o multi.o menu.o fonction.o persoo.o persoo2.o -o prog -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
main.o:main.c
	gcc -c main.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
