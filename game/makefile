	gcc -c option.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
credit.o:credit.c
	gcc -c credit.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
text.o:text.c
	gcc -c text.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
perso.o:perso.c
	gcc -c perso.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
single.o:single.c
	gcc -c single.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
multi.o:multi.c
	gcc -c multi.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
menu.o:menu.c
	gcc -c menu.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
perso2.o:perso2.c
	gcc -c perso2.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
fonction.o:fonction.c
	gcc -c fonction.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
persoo.o:persoo.c
	gcc -c persoo.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
