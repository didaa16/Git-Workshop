prog:multi.o fonction.o perso.o Background.o main.o init.o text.o start.o option.o credit.o single.o menu.o 
	gcc multi.o fonction.o perso.o Background.o main.o init.o text.o start.o option.o credit.o single.o menu.o -o prog -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
main.o:main.c
	gcc -c main.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
init.o:init.c
	gcc -c init.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
start.o:start.c
	gcc -c start.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
option.o:option.c
	gcc -c option.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
credit.o:credit.c
	gcc -c credit.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
text.o:text.c
	gcc -c text.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
single.o:single.c
	gcc -c single.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
multi.o:multi.c
	gcc -c multi.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
menu.o:menu.c
	gcc -c menu.c -lSDL -lSDL_ttf -lSDL_image -lSDL_mixer -g
Background.o:Background.c
	gcc -c Background.c -g
perso.o:perso.c
	gcc -c perso.c -g
fonction.o:fonction.c
	gcc -c fonction.c
	
